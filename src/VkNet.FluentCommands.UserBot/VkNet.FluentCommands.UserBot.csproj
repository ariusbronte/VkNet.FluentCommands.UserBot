<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.1;netstandard2.0;net47</TargetFrameworks>
        <Version>1.1.0</Version>
        <Description>
            Extension for VkNet to quickly create bots.
        </Description>
        <Authors>ariusbronte</Authors>
        <RepositoryType>GIT</RepositoryType>
        <RepositoryUrl>https://github.com/ariusbronte/VkNet.FluentCommands.UserBot</RepositoryUrl>
        <PackageId>VkNet.FluentCommands.UserBot</PackageId>
        <PackageTags>vknet;vk;api;bot;user</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="VkNet.AudioBypassService" Version="1.5.3" />
    </ItemGroup>
</Project>
